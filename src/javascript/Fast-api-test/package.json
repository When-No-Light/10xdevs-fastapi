{
    "name": "fast-api-test",
    "version": "0.0.1",
    "description": "A Quasar Project",
    "productName": "UI for fast api testing",
    "author": "Vadym Stakhov <vadym.stakhov@rd.technosystem.pl>",
    "private": true,
    "scripts": {
        "lint": "eslint --max-warnings=0 --ext .js,.ts,.vue ./",
        "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
        "test": "echo \"No test specified\" && exit 0",
        "dev": "quasar dev",
        "build": "quasar build",
        "generate-client": "openapi --input ./.openapi_schemas/fast_api_test.json --output ./src/client --client axios --useOptions --useUnionTypes",
        "fmt": "pprettier --write \"src/**/*.{js,jsx,ts,tsx,json,vue}\" \"./*.{js,jsx,ts,tsx,json,vue}\" \"!src/client/**/*.{js,jsx,ts,tsx,json,vue}\"",
        "format-check": "pprettier --check \"src/**/*.{js,jsx,ts,tsx,json,vue}\" \"./*.{js,jsx,ts,tsx,json,vue}\" \"!src/client/**/*.{js,jsx,ts,tsx,json,vue}\""
    },
    "dependencies": {
        "@mixer/parallel-prettier": "^2.0.3",
        "@quasar/extras": "^1.16.4",
        "axios": "^1.6.5",
        "quasar": "^2.8.0",
        "vue": "^3.2.29",
        "vue-request": "^2.0.4",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@quasar/app-vite": "^1.4.3",
        "@types/node": "^12.20.21",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "autoprefixer": "^10.4.2",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-vue": "^9.0.0",
        "openapi-typescript-codegen": "^0.27.0",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "engines": {
        "node": "^20 || ^18 || ^16",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
